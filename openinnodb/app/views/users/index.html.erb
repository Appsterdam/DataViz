<h1>Listing users</h1>

<table>
  <tr>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>Twitterid</th>
    <th>Emailadd</th>
    <th>Urladd</th>
    <th>Origin</th>
    <th>Location</th>
    <th>Age</th>
    <th>Background</th>
    <th>Yearsofexp</th>
    <th>Sex</th>
    <th>Education</th>
    <th>Mentor</th>
    <th>Income</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.firstname %></td>
    <td><%= user.lastname %></td>
    <td><%= user.twitterid %></td>
    <td><%= user.emailadd %></td>
    <td><%= user.urladd %></td>
    <td><%= user.origin %></td>
    <td><%= user.location %></td>
    <td><%= user.age %></td>
    <td><%= user.background %></td>
    <td><%= user.yearsofexp %></td>
    <td><%= user.sex %></td>
    <td><%= user.education %></td>
    <td><%= user.mentor %></td>
    <td><%= user.income %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
